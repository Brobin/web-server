<html>
	<head>
		<title>Swag</title>
	</head>
	<body>
		<div class="container">
			{
				string user = "Tobin";
				string foo = "Great Template!!!";
				DateTime dt = DateTime.Now;
			}
			<h1>@{user}'s @{foo}</h1>
			<p>The current time is @{dt.ToString("MMM dd, yyyy hh:mm:ss")}</p>

			<p>The value of the parameter user is @{request["user"]}</p>

			<p>
			{
				for(int i = 0; i < 10; i++)
				{
					wout.WriteLine(i);
				}
			}
			</p>
		</div>
	</body>
</html>