<html>
	<head>
		<title>Swag</title>
	</head>
	<body>
		<div class="container">
			{
				string foo = "My Great Template!!!";
				DateTime dt = DateTime.Now;
				int x = 0;
				for(int i = 0; i < 10; i++)
				{
					x++;
					wout.WriteLine(x);
				}
			}
